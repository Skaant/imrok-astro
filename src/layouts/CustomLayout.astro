---
import Layout from "nebula-atoms-astro/Layout";
import Navbar from "../components/Navbar.astro";

export interface Props {
  title: string;
}

const { title } = Astro.props;
---

<Layout title={title}>
  <Navbar slot="navbar" />
  <div id="container" class="py-5">
    <slot />
  </div>
</Layout>
